   "source": [
    "# Crie um programa que converta uma temperatura em graus Celsius para Fahrenheit. A fórmula é: F=C*9/5+32."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "9a7744eb",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "10\n"
     ]
    }
   ],
   "source": [
    "celsius = float(input())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "a176f5af",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "A temperatura 10.0C é equivalente a 50.0F.\n"
     ]
    }
   ],
   "source": [
    "fahrenheit = (celsius*9/5) + 32\n",
    "\n",
    "print(f'A temperatura {celsius}C é equivalente a {fahrenheit}F.')"
   ]
